{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "description": {
            "type": "string"
        },
        "startTime": {
            "type": "string",
            "format": "date-time"
        },
        "endTime": {
            "type": "string",
            "format": "date-time"
        },
        "address": {
            "type": "string"
        },
        "cityId": {
            "type": "number"
        },
        "isPrivate": {
            "type": "boolean"
        },
        "maxMembers": {
            "type": "number",
            "minimum": 1
        }
    },
    "additionalProperties": false,
    "required": ["title", "startTime", "endTime"]
}
