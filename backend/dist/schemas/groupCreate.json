{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "startTime": {
            "type": "string",
            "format": "date-time"
        },
        "endTime": {
            "type": "string",
            "format": "date-time"
        },
        "location": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": ["title", "description", "startTime", "endTime", "location"]
}
